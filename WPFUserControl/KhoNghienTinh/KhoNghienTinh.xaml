<UserControl x:Class="WPFUserControl.KhoNghienTinh"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:gif="http://wpfanimatedgif.codeplex.com"
             xmlns:local="clr-namespace:WPFUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <local:ConvertToVisibility x:Key="convertToVisibility" />
    </UserControl.Resources>
    <Viewbox>
        <Grid>
            <!-- LABEL -->
            <Label Content="Xi lanh" Foreground="Orange" FontSize="50" FontFamily="Microsoft Sans Serif" FontWeight="Bold" Margin="23,1918,2415,114"/>
            <Label Content="1" Foreground="Orange" FontSize="50" FontFamily="Microsoft Sans Serif" FontWeight="Bold" Margin="-46,1650,2620,381"/>
            <Label Content="2" Foreground="Orange" FontSize="50" FontFamily="Microsoft Sans Serif" FontWeight="Bold" Margin="-44,1813,2611,219"/>
            <Label Content="Vít tải ra liệu" Foreground="Orange" FontSize="50" FontFamily="Microsoft Sans Serif" FontWeight="Bold" Margin="1562,1986,755,40"/>
            <Label Content="Cấp liệu" Foreground="Orange" FontSize="50" FontFamily="Microsoft Sans Serif" FontWeight="Bold" Margin="1975,771,421,1261"/>
            <Label Content="1" Foreground="Orange" FontSize="50" FontFamily="Microsoft Sans Serif" FontWeight="Bold" Margin="2059,852,516,1179"/>
            <Label Content="2" Foreground="Orange" FontSize="50" FontFamily="Microsoft Sans Serif" FontWeight="Bold" Margin="2062,993,510,1038"/>
            <!-- 0. ANH NEN -->

            <Image Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh.png" VerticalAlignment="Top" HorizontalAlignment="Left"/>

            <!-- 1. XI LANH 1 -->

            <Image x:Name="xiLanh1Gif" Visibility="Visible" gif:ImageBehavior.AnimatedSource="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_LuotKhoChay.gif" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="298,1595,0,0" Width="1571"/>
            <Image x:Name="xiLanh1" Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_XiLanhChay.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="-1,1661,0,0" Width="279"/>

            <Image x:Name="xiLanh1Tien" Visibility="Visible" Source="/WPFUserControl;component/Image/Symbol/VitTaiThuanXiLanhTien.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="2034,1658,0,0" Height="61"/>
            <Image x:Name="xiLanh1Lui" Visibility="Visible" Source="/WPFUserControl;component/Image/Symbol/VitTaiNghichXiLanhLui.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="2034,1658,0,0" Height="61"/>

            <!-- 2. XI LANH 2 -->

            <Image x:Name="xiLanh2Gif" Visibility="Visible" gif:ImageBehavior.AnimatedSource="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_LuotKhoNgoaiChay.gif" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="298,1760,0,0" Width="1574"/>
