                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="90"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>

            <Image Visibility="Visible" Source="/WPFUserControl;component/Image/Symbol/Alarm.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="80,916,0,0"/>

            <!-- 4. VIT TAI CAP LIEU 2 -->

            <Image x:Name="vitTaiCapLieu2" Visibility="Visible" gif:ImageBehavior.AnimatedSource="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_VitTaiCapLieu2Chay.gif" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="844,1039,0,0" Width="1132"/>

            <Image x:Name="mortorVitTai2" Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_MotorVitTaiCapLieuChay.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="1990,1042,-3,0"/>
            <!--<Image Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_MotorVitTaiCapLieuLoi.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="1986,1045,-14,0"/>-->

            <Image x:Name="mortorVitTai2ChayNghich" Visibility="Visible" VerticalAlignment="Top" HorizontalAlignment="Left" 
                   Source="/WPFUserControl;component/Image/Symbol/MotorChayNghich.png" Margin="2227,1037,0,0" Height="57" >
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="90"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>

            <Image x:Name="mortorVitTai2ChayThuan" Visibility="Visible" VerticalAlignment="Top" HorizontalAlignment="Left" 
                   Source="/WPFUserControl;component/Image/Symbol/MotorChayThuan.png" Margin="2227,1037,0,0" Height="57">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="90"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>

            <Image Visibility="Visible" Source="/WPFUserControl;component/Image/Symbol/Alarm.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="80,1055,0,0"/>


            <!-- 5. VIT TAI RA LIEU -->

            <Image x:Name="vitTaiRaLieuGif" Visibility="Visible" gif:ImageBehavior.AnimatedSource="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTho_VitTaiRaLieuChay.gif" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="1875,1611,0,-8" Height="497"/>

            <Image x:Name="motorVitTaiRaLieu"  Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_MotorVitTaiRaLieuChay.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="1889,1986,0,-1"/>
            <!--<Image Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_MotorVitTaiRaLieuLoi.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="1892,1990,0,-2"/>-->


            <!-- 6. BOM DAU -->

            <Image x:Name="bomDau1" Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_MotorBomDauChay.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="46,1437,0,0"/>
            <!--<Image Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_MotorBomDauLoi.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="41,1439,0,0"/>-->

            <!-- 7. BON DAU -->

            <Image Visibility="Visible" Source="/WPFUserControl;component/Image/Symbol/Alarm.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="80,1307,0,0"/>

            <!-- 8. CYCLON -->

            <Image x:Name="Cylone1" Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_CyclonBaoDay.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="1455,1,0,0" Height="835" Width="370"/>
            <Image x:Name="Cylone2" Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_CyclonBaoDay.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="942,135,0,0" Height="835" Width="370"/>


            <!-- 10. ROTOVAN 1 -->

            <Image Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_RotovanChay.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="1473,813,0,0"/>
            <Image Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_RotovanLoi.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="1474,815,0,0"/>


            <!-- 11. ROTOVAN 2 -->

            <Image Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_RotovanChay.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="958,948,0,0"/>
            <Image Visibility="Visible" Source="/WPFUserControl;component/Image/KhoNghienTinh/KhoNghienTinh_RotovanLoi.png" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="960,950,0,0"/>



        </Grid>
    </Viewbox>
</UserControl>
